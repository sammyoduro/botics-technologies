<div class="row">
  <div class=" col-md-4 ">
      <main class="card">
        <div class="card-body">
          <form  action="/areacode97/admin/outofbound/admin/adminsregistration" method="post">
            <div class="form-group">
          		<label for="pnumber">First name</label>
          		<input type="text" id="fname" name="fname" class="form-control">
              <% if(typeof fnameValidationErr != 'undefined'){ %> <small class="text-danger"><%= fnameValidationErr %></small> <%}%>
          	</div>
            <div class="form-group">
          		<label for="lname">Last name</label>
          		<input type="text" id="lname" name="lname" class="form-control">
              <% if(typeof lnameValidationErr != 'undefined' ){ %> <small class="text-danger"><%= lnameValidationErr %></small> <%}%>
          	</div>
            <div class="form-group">
          		<label for="pnumber">Phone number</label>
          		<input type="text" id="pnumber" name="pnumber" class="form-control">
              <% if(typeof pnumberValidationErr != 'undefined' ){ %><p class="text-danger"> <%= pnumberValidationErr %></p> <%}%>
          	</div>
            <div class="form-group">
              <label for="opnumber">Other phone number</label>
              <input type="text" id="opnumber" name="opnumber" class="form-control">
            </div>
            <div class="form-group">
              <label for="opnumber">User type</label>
              <select class="form-control" name="usertype">
                <option value="ADMIN">Administrator</option>
                <option value="SUPERADMIN">Super Administrator</option>
              </select>
            </div>
          	<div class="form-group">
          		<label>Email address</label>
          		<input type="text" id="email" name="email" class="form-control">
              <% if(typeof emailValidationErr != 'undefined' ){ %><p class="text-danger"> <%= emailValidationErr %></p> <%}%>
          	</div> <!-- form-group end.// -->
              <div class="form-group">
                  <button type="submit" class="btn btn-primary btn-block"> Register  </button>
              </div> <!-- form-group// -->
          </form>
        </div>
      </main>
    </div>
  <div class=" col-md-8 ">
      <main class="card">
        <div class="card-body">
          <h6>REGISTERED ADMINISTRATORS LIST</h6>
<%- messages('message',locals) %>
          <div class="list-wrapper">
            <% if(admins){ %>
              <table class="table table-inbox table-hover">
                <tbody>

              <% admins.forEach(function(data){ %>


              <tr>
                <td class="view-message "> <small><%=data.userType %></small> </td>
                  <td class="view-message"> <small><%=data.firstname %> <%=data.lastname %></small> </td>
                  <td class="view-message"><small><%=data.email %></small></td>
                  <td class="view-message"> <small>contact(<%=data.phone_num %>/<%=data.Otherphone_num %>)</small> </td>
                  <td class="view-message"> <button type="button" class="btn btn-sm btn-primary">reset password</button> </td>
                  <td class="view-message"><button type="button" class="btn btn-sm btn-danger">del</button></td>
              </tr>

            <%  }) %>
          </tbody>
            </table>
          <% }%>
          </div>
        </div>
      </main>
    </div>

