<div class="row">
<div class="col-xl-12 col-md-12 col-sm-12">
      <main class="card">
        <div class="card-body">
          <div class="row">

    <div class="col-lg-12" style="margin-bottom:10px">
      <div class="col-lg-12 card">
        <div class="col-md-12" style="margin-bottom:10px;padding-top:50px;padding-left:0px;">
              <div class="scrollbar scrollbar-primary">
                <div class="force-overflow printout">
                  <div class="row" id="MyOrder">
                    <% if(orders){%>
                      <% orders.forEach(function(e){%>
                        <div class="col-md-6" style="margin-bottom:10px">
                          <div class="card">
                            <div class="card-header">
                              <% if(e.delivered == 'delivered'){ %>
                                <span class="text-success"> <i class="fa fa-check-circle"></i> <%=e.delivered %></span><br>
                              <% }else{%>
                                <span class="text-primary"> <i class="fa fa-repeat"></i> <%=e.delivered %></span><br>
                              <%}%>
                              name: <span class="text-default"><%=e.name %></span><br>
                              order Id: <span class="text-default"><%=e.order_id %></span><br>
                              Address: <span class="text-default"><%=e.address %></span>
                              <h6 class="pull-right title">ordered <%= moment(e.timestamp).fromNow()%></h6>
                            </div>
                            <div class="card-body">
                              <div class="list-group list-group-flush">
                              <% for(var i=0;i< e.items.length;i++){%>
                                <a href="/pages/store-front/product_detail/p/?q=<%= e.items[i].item._id %>" class="list-group-item"><%=e.items[i].item.title%> | <%=e.items[i].qty%> pcs<span class="float-right badge badge-secondary round">Ghc <%=e.items[i].item.price%></span></a>
                              <%}%>
                              </div>
                            </div>
                            <div class="card-footer text-muted">
                              <strong>Total price: Ghc <%=e.cart.totalPrice%></strong>
                            </div>
                          </div>
                        </div>
                      <%})%>
                    <%}else{%>
                      <h1>Orders empty</h1>
                    <%}%>
                  </div>
                </div>
              </div>

            </div>
          </div>
      </div>
    </div>
        </div>
      </main>
    </div>
  </div>
