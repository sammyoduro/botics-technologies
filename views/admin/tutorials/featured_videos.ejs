<script type="text/javascript">
       $(document).ready(function () {

           $('.watch-tuts').on('click', function () {
             var vidurl = $(this).attr('url');
             // alert($(this).attr('url')); modal-body
             $('.modal-body').html('<div class="embed-responsive embed-responsive-16by9"><iframe class="embed-responsive-item" src="'+vidurl+'"allowfullscreen></iframe></div><div class="modal-footer"><button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button></div>')
               $('#watch_tuts').modal();
           });
       });
   </script>
