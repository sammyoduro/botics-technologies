<section class="section-content bg padding-y-sm">
<div class="container">
  <nav class="mb-3">
  <ol class="breadcrumb">
      <li class="breadcrumb-item active" aria-current="/about">Featured videos</li>
  </ol>
  </nav>


  <div class=" col-md-7 offset-md-3 ">
    <main class="card">
      <div class="card-body">
                  <h1>New tutorials</h1> <% if( typeof sucmsg != 'undefined' ){ %> <p class="text-success"> <%= sucmsg %> </p> <%}%>
                  <form  action="/areacode97/admin/outofbound/admin/featured_vid" method="post">
                    <div class="form-group">
                      <label for="vidurl" class="col-form-label">Title</label>
                      <input type="text" class="form-control"  name="title">
                      <% if( typeof title_err != 'undefined' ){ %> <p class="text-danger"> <%= title_err %> </p> <%}%>
                    </div>
                    <div class="form-group">
                      <label for="vid_image" class="col-form-label">Featured video image url</label>
                      <input type="text" class="form-control"  name="vid_image">
                      <% if( typeof vid_image_err != 'undefined' ){ %> <p class="text-danger"> <%= vid_image_err %> </p> <%}%>
                    </div>
                    <div class="form-group">
                      <label for="vid_imgurl" class="col-form-label">Featured video URL:</label>
                      <input type="text" class="form-control"  name="vid_url">
                      <% if( typeof vid_tut_err != 'undefined' ){ %> <p class="text-danger"> <%= vid_tut_err %> </p> <%}%>
                    </div>
                    <div class="form-group">
                      <button type="submit" class="form-control btn btn-bg btn btn-info" >Upload</button>
                    </div>
                  </form>
    </div>
    </main>
  </div>
  <div class=" col-md-12" style="margin-top:10px">
        <h6>TUTORIALS LIST</h6>
        <div class="list-wrapper">
          <div class="card">
          <div class="card-body">
          	<div class="row ">
              <ul class="list-unstyled video-list-thumbs row">
                  <% if (videos){ %>
                    <% for(var i=0; i< videos.length; i++){ %>

                      <li class="col-lg-3 col-sm-4 col-xs-6">
                        <a href="" title="<%= videos[i].title %>">
                          <img src="<%= videos[i].video_img_url %>" alt="turial video preview" class="img-responsive" height="130px" style="width:100%"/>
                          <h2><%= videos[i].title %></h2>
                          <i class="fa fa-play-circle-o"></i>
                        </a>
                        <!-- <button type="button" class="btn btn-danger btn-sm" >>>del</button> -->
                        <a href="/areacode97/admin/outofbound/admin/featured_vid/delete/?id=<%= videos[i].id %>" class="del">>>del</a>
                      </li>
                    <% } %>
                  <% } %>
                </ul>
          	</div> <!-- row.// -->
          </div>
          </div> <!-- card.// -->
        </div>
  </div>

